{
    "model": "sklearn.linear_model.LogisticRegression",
    "classes": [0,1],
    "datasets": {
        "uciml/sms-spam-collection-dataset": {
            "encoding": "iso-8859-1",
            "unpack_name": "spam.csv",
            "columns": {
                "rating": {
                    "label": "v1",
                    "map": {
                        "ham": 1,
                        "spam": 0
                    }
                },
                "text": {
                    "label": "v2"
                }
            }
        },
        "venky73/spam-mails-dataset": {
            "unpack_name": "spam_ham_dataset.csv",
            "columns": {
                "rating": {
                    "label": "label_num"
                },
                "text": {
                    "label": "text"
                }
            }
        },
        "ozlerhakan/spam-or-not-spam-dataset": {
            "unpack_name": "spam_or_not_spam.csv",
            "columns": {
                "rating": {
                    "label": "label",
                    "map": {
                        "0": 1,
                        "1": 0
                    }
                },
                "text": {
                    "label": "email"
                }
            }
        }
    }
}
